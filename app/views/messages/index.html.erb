<div class="row">
      <div class="col-sm-2">
      </div>
      <div class="col-sm-8">
            <p id="notice"><%= notice %></p>

            <h1>Messages</h1>

            <p>
              Download:
              <%= link_to "CSV", messages_path(format: "csv") %> <!-- |
              <%= link_to "Excel", messages_path(format: "xls") %> -->
            </p>

            <table class="table table-hover">
              <thead>
                <tr>
                  <th>Type</th>
                  <th>Repeatindicator</th>
                  <th>Mmsi</th>
                  <th>Receiveddate</th>
                  <th colspan="3"></th>
                </tr>
              </thead>

              <tbody>
                <% @messages.each do |message| %>
                  <tr>
                    <td><%= message.type %></td>
                    <td><%= message.repeatIndicator %></td>
                    <td><%= message.mmsi %></td>
                    <td><%= message.receivedDate %></td>
                    <td><%= link_to 'Show', message %></td>
                    <td><%= link_to 'Edit', edit_message_path(message) %></td>
                    <td><%= link_to 'Destroy', message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
<!--             <div align="right">
            <%= button_to "Save", csv_messages_path, :class => "btn btn-success", type: "submit" %>
            </div> -->
            <br>

<!--             <%= link_to 'New Message', new_message_path %>
 -->      </div>
       <div class="col-sm-2">
      </div>     
</div>